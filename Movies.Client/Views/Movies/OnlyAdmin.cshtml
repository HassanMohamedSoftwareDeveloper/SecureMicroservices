@model Movies.Client.Models.UserInfoViewModel
@{
    ViewData["Title"] = "Only Admin";
}

<h1>Only Admin</h1>

<table class="table table-bordered ">
    <thead>
        <tr>
            <th>Claim Type</th>
            <th>Claim Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.UserInfoDictionary)
        {
            <tr>
                <td>@item.Key</td>
                <td>@item.Value</td>
            </tr>
        }
    </tbody>
</table>
